02/7/25
aws eks update-kubeconfig --region us-east-2 --name nilesh
kubectl cluster-info
kubectl get nodes
kubectl get pods
kubectl run nilesh
kubectl get pods -o wide
kubectl expose pod nilesh --type=NodePort --port 80
kubectl get service
kubectl get namespace
kubectl run pod --image=nginx -n dev 
kubectl create namespace dev
kubectl api-resources
kubectl apply -f pod.yaml
